    "egg-onerror": "^2.2.0",
    "egg-redis": "^2.5.0",
    "egg-schedule": "^3.7.0",
    "egg-scripts": "^2.17.0",
    "egg-security": "^2.11.0",
    "egg-sequelize": "^6.0.0",
    "egg-validate": "^2.0.2",
    "encoding-down": "^7.1.0",
    "install": "^0.13.0",
    "lru-cache": "^6.0.0",
    "moment": "^2.29.4",
    "mysql2": "^2.3.3",
