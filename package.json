  "description": "爱发电赞助者管理服务",
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=afdian-sponsor-manage-service",
    "start:docker": "egg-scripts start --title=afdian-sponsor-manage-service",
    "start:prod": "EGG_SERVER_ENV=prod npm start",
